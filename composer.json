{
  "require": {
    "ext-json": "*",
    "ext-pdo": "*",
    "bernard/bernard": "1.0.0-alpha6",
    "php-di/php-di": "^6.2",
    "phpmailer/phpmailer": "^6.1",
    "predis/predis": "^1.1",
    "selective/basepath": "^1.0",
    "slim/psr7": "^1.2",
    "slim/slim": "4.*",
    "symfony/property-access": "^3.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.4"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/",
      "Helpers\\": "helpers/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Test\\": "tests/"
    }
  },
  "config": {
    "process-timeout": 0,
    "sort-packages": true
  }
}
